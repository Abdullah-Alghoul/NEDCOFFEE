<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    	
    	<!-- Outsource Receipt -->
		<record id="sequence_outsource_receipt" model="ir.sequence">
            <field name="name">Outsource Receipt</field>
            <field name="code">outsource.receipt</field>
            <field eval="4" name="padding"/>
            <field name="prefix">OUTSOUCE-R/</field>
            <field name="suffix">%(y)s</field>
            <field name="rollback_rule">Yearly</field>
        </record>
        
        <!-- Outsource Issue -->
		<record id="sequence_outsource_issue" model="ir.sequence">
            <field name="name">Outsource Issue</field>
            <field name="code">outsource.issue</field>
            <field eval="4" name="padding"/>
            <field name="prefix">OUTSOUCE-I/</field>
            <field name="suffix">s%(y)s</field>
            <field name="rollback_rule">Yearly</field>
        </record>
        
    </data>
</openerp>